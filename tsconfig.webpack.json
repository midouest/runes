{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": false,
    "removeComments": true,
    "baseUrl": "config",
    "outDir": ".",
    "moduleResolution": "node",
    "paths": {
      "*": ["node_modules/*"]
    }
  },
  "include": ["config/*"],
  "exclude": ["node_modules"]
}
